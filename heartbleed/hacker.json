{
    "key": "heartbleedhacker",
    "label": "HeartBleed",
    "description": "Demonstration of the HeartBleed bug in the OpenSSL library",
    "logo": "/asset/png/logos/heartbleed.png",
    "image": {
        "name": "rkalyana/heartbleed-hacker",
        "tags": ["latest"]
    },
    "display": "stack",
    "access": "external",
    "depends": [
        {
            "key": "heartbleedserver",
            "required": true
        },
	{
	    "key": "heartbleedvictim",
	    "required": true
	}
    ],
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "readinessProbe": {
        "type": "tcp",
        "path": "",
        "port": 80,
        "initialDelay": 30,
        "timeout": 500
    },
    "resourceLimits": {
        "cpuMax": 1000,
        "cpuDefault": 100,
        "memMax": 4096,
        "memDefault": 100
    },
	"authRequired": true,
    "info": "https://github.com/cheese-hub/heartbleed/blob/master/README.md"
}
