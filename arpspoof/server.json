{
    "key": "arspoofserver",
    "label": "ArpSpoof Server",
    "description": "",
    "logo": "/asset/png/logos/ndslabs-badge.png",
    "image": {
        "name": "timwang/ieeetrycybserverapacheflaskappsslhstsrewrite:4.0",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
